{
  "name": "knowmatic",
  "version": "1.0.0",
  "description": "Local prompt classification CLI â€” route prompts to the right model tier using on-device ONNX models",
  "type": "module",
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "bin": {
    "knowmatic": "./dist/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
